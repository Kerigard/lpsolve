{
    "name": "kerigard/lpsolve",
    "description": "LPSolve extension as simple PHP library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislav Sidelnikov",
            "email": "kerihoros@gmail.com"
        },
        {
            "name": "makao",
            "email": "makao@makao.pl"
        }
    ],
    "homepage": "https://github.com/kerigard/lpsolve",
    "keywords": [
        "linear",
        "programming",
        "solver",
        "solve",
        "simplex",
        "math",
        "php"
    ],
    "require": {
        "php": ">=5.6",
        "ext-lpsolve": ">=5.5.0.8"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Kerigard\\LPSolve\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kerigard\\LPSolve\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}